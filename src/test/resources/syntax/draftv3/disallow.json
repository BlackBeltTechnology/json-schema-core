{
    "valueTests": [
        {
            "schema": {
                "disallow": "string"
            },
            "valid": true
        },
        {
            "schema": {
                "disallow": "any"
            },
            "valid": true
        },
        {
            "schema": {
                "disallow": [ "boolean", { "$ref": "#/schema2" } ]
            },
            "valid": true
        },
        {
            "schema": {
                "disallow": false
            },
            "valid": false,
            "message": "incorrectType",
            "msgData": {
                "expected": [ "array", "string" ],
                "found": "boolean"
            },
            "msgParams": [ "found", "expected" ]
        },
        {
            "schema": {
                "disallow": "hammer"
            },
            "valid": false,
            "message": "incorrectPrimitiveType",
            "msgData": {
                "valid": [ "array", "boolean", "integer", "null", "number", "object", "string" ],
                "found": "hammer"
            }
        },
        {
            "schema": { "disallow": [ "boolean", 32.2, "object" ] },
            "valid": false,
            "message": "incorrectElementType",
            "msgData": {
                "index": 1,
                "expected": [ "object", "string" ],
                "found": "number"
            }
        },
        {
            "schema": { "disallow": [ "array", "strin", "object" ] },
            "valid": false,
            "message": "incorrectPrimitiveType",
            "msgData": {
                "index": 1,
                "valid": [ "array", "boolean", "integer", "null", "number", "object", "string" ],
                "found": "strin"
            }
        },
        {
            "schema": {
                "disallow": [ "string", "string" ]
            },
            "valid": false,
            "message": "elementsNotUnique",
            "msgData": {}
        }
    ],
    "pointerTests": [
        {
            "schema": { "disallow": "string" },
            "pointers": []
        },
        {
            "schema": { "disallow": [ "string", "number" ] },
            "pointers": []
        },
        {
            "schema": { "disallow": [ "null", {}, "string", 1, false ] },
            "pointers": [ "/disallow/1" ]
        },
        {
            "schema": { "disallow": [ {}, {}, "string", {} ] },
            "pointers": [ "/disallow/0", "/disallow/1", "/disallow/3" ]
        }
    ]
}